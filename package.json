{
  "name": "react-carousel",
  "version": "0.1.0",
  "description": "a Simple react carousel component.",
  "main": "lib",
  "scripts": {
    "start": "webpack-dev-server --progress --profile --colors",
    "lint": "eslint --ext js --ext jsx src || exit 0",
    "dev": " webpack-dashboard -- webpack-dev-server",
    "test": "export NODE_ENV=test && jest",
    "build": "babel src -d lib --copy-files"
  },
  "keywords": [
    "react",
    "carousel",
    "slider",
    "slide",
    "image",
    "component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chuck-durst/react-carousel"
  },
  "author": "chuck-durst",
  "license": "MIT",
  "homepage": "https://github.com/chuck-durst/react-carousel#readme",
  "dependencies": {
    "node-sass": "^4.3.0",
    "prop-types": "^15.6.0",
    "react": "15.6.1",
    "react-dom": "15.4.2",
    "sass-loader": "^6.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",

    "babel-core": "^6.23.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-runtime": "^6.22.0",
    "css-loader": "0.26.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.3.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.26.0",
    "jest": "^20.0.4",
    "loglevel": "^1.4.1",
    "postcss-loader": "^1.2.2",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.6.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "^3.3.0",
    "webpack-cleanup-plugin": "^0.4.2",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.4.1"
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "moduleNameMapper": {
      "\\.(jpe?g|png|gif|eot|otf|webp|svg|ttf|woff2?|mp[34]|webm|wav|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|s[ac]ss|styl)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  }
}
